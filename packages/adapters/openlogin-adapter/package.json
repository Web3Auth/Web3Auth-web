{
  "name": "@web3auth/openlogin-adapter",
  "version": "5.1.1",
  "description": "openlogin adapter for web3auth",
  "keywords": [
    "web3Auth/openlogin-adapter",
    "web3Auth",
    "multichainWallet",
    "blockchain",
    "ethereum",
    "solana"
  ],
  "main": "dist/openloginAdapter.cjs.js",
  "module": "dist/openloginAdapter.esm.js",
  "unpkg": "dist/openloginAdapter.umd.min.js",
  "jsdelivr": "dist/openloginAdapter.umd.min.js",
  "types": "dist/types/index.d.ts",
  "author": "Torus Labs",
  "homepage": "https://github.com/Web3Auth/Web3Auth/tree/master/packages/adapters/openlogin-adapter#readme",
  "license": "ISC",
  "scripts": {
    "test": "mocha --config ../../../.mocharc.json test/**.ts",
    "test-debugger": "mocha --config ../../../.mocharc.json --inspect-brk test/**.ts",
    "dev": "torus-scripts start",
    "build": "torus-scripts build",
    "lint": "eslint --fix 'src/**/*.ts'",
    "prepack": "yarn run build",
    "pre-commit": "lint-staged --cwd ."
  },
  "files": [
    "dist",
    "src"
  ],
  "peerDependencies": {
    "@babel/runtime": "^7.x",
    "@solana/web3.js": "^1.x"
  },
  "devDependencies": {
    "@types/lodash.merge": "^4.6.7"
  },
  "dependencies": {
    "@toruslabs/openlogin": "../../../../OpenLoginSdk/packages/openlogin",
    "@toruslabs/openlogin-ed25519": "../../../../OpenLoginSdk/packages/openlogin-ed25519",
    "@toruslabs/openlogin-utils": "../../../../OpenLoginSdk/packages/openlogin-utils",
    "@web3auth/base": "^5.1.0",
    "@web3auth/base-provider": "^5.1.1",
    "@web3auth/ethereum-provider": "^5.1.1",
    "@web3auth/solana-provider": "^5.1.1",
    "lodash.merge": "^4.6.2"
  },
  "lint-staged": {
    "!(*d).ts": [
      "eslint --cache --fix",
      "prettier --write"
    ]
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Web3Auth/Web3Auth.git"
  },
  "bugs": {
    "url": "https://github.com/Web3Auth/Web3Auth/issues"
  }
}
