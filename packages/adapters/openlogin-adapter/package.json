    "@toruslabs/openlogin": "^8.1.2",
    "@toruslabs/openlogin-session-manager": "^3.1.1",
    "@toruslabs/openlogin-utils": "^8.1.2",
    "@web3auth/base": "^8.6.2",
    "@web3auth/base-provider": "^8.6.2",
    "bs58": "^5.0.0",
    "lodash.merge": "^4.6.2"
