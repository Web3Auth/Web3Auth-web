{
  "name": "@web3auth/farcaster-adapter",
  "version": "0.1.0",
  "description": "farcaster login adapter for web3auth",
  "keywords": [
    "web3Auth/farcaster-adapter",
    "web3Auth",
    "blockchain",
    "ethereum",
    "farcaster"
  ],
  "main": "dist/farcasterAdapter.cjs.js",
  "module": "dist/farcasterAdapter.esm.js",
  "unpkg": "dist/farcasterAdapter.umd.min.js",
  "jsdelivr": "dist/farcasterAdapter.umd.min.js",
  "types": "dist/types/index.d.ts",
  "author": "Torus Labs",
  "homepage": "https://github.com/Web3Auth/Web3Auth/tree/master/packages/adapters/farcaster-adapter#readme",
  "license": "ISC",
  "scripts": {
    "test": "mocha --config ../../../.mocharc.json test/**.ts",
    "test-debugger": "mocha --config ../../../.mocharc.json --inspect-brk test/**.ts",
    "dev": "torus-scripts start",
    "build": "torus-scripts build",
    "lint": "eslint --fix 'src/**/*.ts'",
    "prepack": "npm run build",
    "pre-commit": "lint-staged --cwd ."
  },
  "files": [
    "dist",
    "src"
  ],
  "peerDependencies": {
    "@babel/runtime": "^7.x"
  },
  "dependencies": {
    "@web3auth/base": "^7.3.1",
    "@web3auth/base-evm-adapter": "^7.3.1",
    "@web3auth/ethereum-provider": "file:../../providers/ethereum-provider"
  },
  "lint-staged": {
    "!(*d).ts": [
      "eslint --cache --fix",
      "prettier --write"
    ]
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Web3Auth/Web3Auth.git"
  },
  "bugs": {
    "url": "https://github.com/Web3Auth/Web3Auth/issues"
  },
  "engines": {
    "node": ">=18.x",
    "npm": ">=9.x"
  }
}
