{
  "name": "@web3auth/wallet-connect-connector",
  "version": "8.7.0",
  "description": "wallet connect connector for web3auth",
  "keywords": [
    "@web3auth/wallet-connect-connector",
    "web3auth",
    "multichainWallet",
    "blockchain",
    "ethereum",
    "walletconnect"
  ],
  "main": "dist/walletConnectConnector.cjs.js",
  "module": "dist/walletConnectConnector.esm.js",
  "unpkg": "dist/walletConnectConnector.umd.min.js",
  "jsdelivr": "dist/walletConnectConnector.umd.min.js",
  "types": "dist/types/index.d.ts",
  "author": "Torus Labs",
  "homepage": "https://github.com/Web3Auth/Web3Auth/tree/main/packages/connectors/wallet-connect-connector#readme",
  "license": "ISC",
  "scripts": {
    "test": "mocha --config ../../../.mocharc.json test/**.ts",
    "test-debugger": "mocha --config ../../../.mocharc.json --inspect-brk test/**.ts",
    "dev": "torus-scripts start",
    "build": "torus-scripts build",
    "lint": "eslint --fix 'src/**/*.ts'",
    "prepack": "npm run build",
    "pre-commit": "lint-staged --cwd ."
  },
  "files": [
    "dist"
  ],
  "peerDependencies": {
    "@babel/runtime": "^7.x",
    "@walletconnect/sign-client": "^2.x",
    "@walletconnect/types": "^2.x",
    "@walletconnect/utils": "^2.x"
  },
  "dependencies": {
    "@metamask/rpc-errors": "^6.3.0",
    "@toruslabs/openlogin-jrpc": "^8.1.1",
    "@web3auth/base": "^8.7.0",
    "@web3auth/base-evm-connector": "^8.7.0",
    "@web3auth/base-provider": "^8.7.0",
    "@web3auth/ethereum-provider": "^8.7.0",
    "lodash.merge": "^4.6.2"
  },
  "devDependencies": {
    "@types/lodash.merge": "^4.6.9",
    "@walletconnect/sign-client": "^2.13.3",
    "@walletconnect/types": "^2.13.3",
    "@walletconnect/utils": "^2.13.3"
  },
  "lint-staged": {
    "!(*d).ts": [
      "eslint --cache --fix",
      "prettier --write"
    ]
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Web3Auth/Web3Auth.git"
  },
  "bugs": {
    "url": "https://github.com/Web3Auth/Web3Auth/issues"
