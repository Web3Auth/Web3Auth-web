    "@toruslabs/openlogin-session-manager": "../../../OpenLoginSdk/packages/session-manager/openlogin-session-manager",
    "@toruslabs/metadata-helpers": "^3.1.0",
    "@toruslabs/tss-client": "^1.5.0-alpha.0",
		"@toruslabs/tss-lib": "^1.2.1",
    "@web3auth/base": "^5.1.0",
    "@web3auth/base-provider": "^5.1.1",
    "@web3auth-mpc/ethereum-provider": "^2.1.22",
